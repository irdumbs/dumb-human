    "Christian Petersen (http://cbp.io) <chrs@cbp.io>",
    "irdumb"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/irdumbs/dumb-human/issues"
  },
  "homepage": "https://github.com/irdumbs/dumb-human#readme",
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.1.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "rimraf": "^2.4.3",
    "standard": "^5.3.1",
    "tape": "^4.2.2",
    "watch": "^0.16.0"
  }
