{
  "name": "dumb-human",
  "version": "0.0.1",
  "description": "A modular twitch bot",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --optional runtime src -d dist",
    "watch": "watch 'npm run build' src test",
